SELECT * FROM actor
WHERE first_name LIKE "%A;



SELECT * FROM actor
WHERE first_name LIKE "A%"
OR first_name LIKE "E%";



 SELECT actor_id,first_name,last_name FROM actor
 WHERE actor_id NOT LIKE "5";
 
 
 
SELECT * FROM address

where city_id = "_50"
and not district = 'QLD' 
or district = 'Alberta';
