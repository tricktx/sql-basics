SELECT * FROM payment
where amount = 2.99;



SELECT rental_id FROM payment
WHERE rental_id = 50;



SELECT * FROM payment
WHERE rental_id IS NULL;
 
 
 
SELECT actor_id FROM actor
WHERE actor_id >= 100
ORDER BY;



SELECT COUNT (*)FROM payment
WHERE rental_id > 300;
 
 
 
 SELECT actor_id,first_name,last_name FROM actor
 WHERE actor_id < 50
 ORDER BY actor_id asc;
 
 
 
SELECT City FROM Station
WHERE (ID % 2) = 0;
