AND == E, OR == OU, NOT == N√ÉO, IN == DENTRO, VALORES ENTRE == BETWEEN


SELECT * FROM payment
WHERE NOT staff_id = 1 AND NOT amount = 0.99 AND customer_id < 10


SELECT * FROM address
WHERE city_id = 50
AND NOT district = 'QLD' 
OR district = 'Alberta'


SELECT * FROM address
WHERE district IN ('Alberta', 'California') 



SELECT * FROM payment
WHERE amount BETWEEN 1.99 AND 3.99
ORDER BY amount asc
